Sub Macro1()
'
' Macro1 Macro
'
' Keyboard Shortcut: Ctrl+e
'
Dim erow As Long, lastrow As Long, lastcolumn As Long
Dim WS As Worksheet
Dim i As Long
Workbooks.Open Filename:="F:\MANISHA\2016-17\PROJECTION SATISH\PROJECTION CELLAGE 2016-17\AP HYDERABAD.xlsx"
Worksheets("RBM HYDERABAD").Activate
lastrow = ActiveSheet.Cells(Rows.Count, 1).End(xlUp).Row
Range("A3:R" & lastrow).Select
Selection.Copy
lastcolumn = ActiveSheet.Cells(1, Columns.Count).End(xlToLeft).Column
ActiveWorkbook.Close


erow = ActiveSheet.Cells(Rows.Count, 1).End(xlUp).Offset(3, 0).Row
Cells(erow, 1).Select
ActiveSheet.Paste
Set Rng = Range(Cells(erow, 1), Cells(lastrow, "R"))
With Rng.Borders
.LineStyle = xlContinuous
.Color = vbBlack
.Weight = xlThin
End With

'AP KURNOOL

Workbooks.Open Filename:="F:\MANISHA\2016-17\PROJECTION SATISH\PROJECTION CELLAGE 2016-17\AP KURNOOL.xlsx"
Worksheets("RBM KURNOOL").Activate
lastrow = ActiveSheet.Cells(Rows.Count, 1).End(xlUp).Row
Range("A3:R" & lastrow).Select
Selection.Copy
lastcolumn = ActiveSheet.Cells(1, Columns.Count).End(xlToLeft).Column
ActiveWorkbook.Close

Sheets.Add.Name = "AP KURNOOL"
Worksheets("AP KURNOOL").Activate
erow = ActiveSheet.Cells(Rows.Count, 1).End(xlUp).Offset(3, 0).Row
Cells(erow, 1).Select
ActiveSheet.Paste
Set Rng = Range(Cells(erow, 1), Cells(lastrow, "R"))
With Rng.Borders
.LineStyle = xlContinuous
.Color = vbBlack
.Weight = xlThin
End With

